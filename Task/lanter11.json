        "autotime_zq",
        "youth_start",
        "youth_look",
        "cashurl_zq",
        "cashbody_zq",
        "zqcash",
        "zqcard"
      ],
      "repo" : "https://github.com/Sunert/Scripts/blob/master/Task/youth.js",
      "id" : "youth",
      "script" : "https://raw.githubusercontent.com/Sunert/Scripts/master/Task/youth.js",
      "settings" : [
        {
          "id" : "zqcard",
          "val" : "false",
          "name" : "打卡赚钱",
